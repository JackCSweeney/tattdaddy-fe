<center>
<h1> Create a new Tattoo </h1>
</center>

<nav>
  <%= link_to "My Profile", artist_path(id: @artist_id) %>
  <%= link_to 'Appointments', artist_appointments_path(id: @artist_id) %>
  <%= link_to 'Sign Out', sign_out_path, method: :delete %>
</nav>

<!%= render partial: "shared/nav_artist"%>

<%= form_with url: "/artists/#{@artist_id}/tattoos", method: :post, local: true, data: {turbo: false} do |f|%>
  <%= f.label :price %>
  <%= f.text_field :price %><br>
  
  <%= f.label :time_estimate %>
  <%= f.text_field :time_estimate %> hours<br>
  
  <%= f.label :img_file %>
  <%= f.file_field :img_file %><br>
  
  <%= f.submit %>
<% end %>